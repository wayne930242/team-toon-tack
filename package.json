{
	"name": "team-toon-tack",
	"version": "1.6.0",
	"description": "Linear task sync & management CLI with TOON format",
	"type": "module",
	"bin": {
		"ttt": "./dist/bin/cli.js",
		"team-toon-tack": "./dist/bin/cli.js"
	},
	"exports": {
		".": "./dist/scripts/utils.js",
		"./utils": "./dist/scripts/utils.js"
	},
	"files": [
		"dist",
		"templates"
	],
	"scripts": {
		"build": "tsc",
		"lint": "biome lint .",
		"format": "biome format --write .",
		"type": "tsc --noEmit",
		"prepublishOnly": "npm run build",
		"release": "npm config set //registry.npmjs.org/:_authToken=$NPM_PUBLISH_KEY && npm publish"
	},
	"dependencies": {
		"@linear/sdk": "^69.0.0",
		"@toon-format/toon": "^2.1.0",
		"prompts": "^2.4.2"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.11",
		"@types/node": "^25.0.3",
		"@types/prompts": "^2.4.9",
		"typescript": "^5.9.3"
	},
	"keywords": [
		"linear",
		"task-management",
		"cli",
		"toon",
		"sync",
		"project-management"
	],
	"author": "wayne930242",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/wayne930242/team-toon-tack.git"
	},
	"engines": {
		"node": ">=18.0.0"
	}
}
